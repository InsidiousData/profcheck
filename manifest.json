{
  "manifest_version": 2,
  "name": "ProfessorCheck",
  "version": "1.0",
  "background": {
    "scripts": ["jquery-3.5.1.slim.min.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.1.slim.min.js", "content.js"]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
